    "panels": [
      {
        "id": 1,
        "title": "âš¡ Real-time Price Stream",
        "type": "timeseries",
        "targets": [
          {
            "datasource": "Athena",
            "rawSql": "SELECT \n  timestamp as time,\n  price as value,\n  symbol as metric\nFROM financial_data.processed_quotes \nWHERE symbol IN ('AAPL', 'GOOGL', 'MSFT', 'AMZN', 'TSLA')\n  AND timestamp >= current_timestamp - interval '5' minute\nORDER BY timestamp",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["last", "max", "min"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "spanNulls": false,
              "fillOpacity": 10,
              "gradientMode": "none",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineStyle": {
                "fill": "solid"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "currencyUSD",
            "decimals": 2
          }
        }
      },
      {
        "id": 2,
        "title": "ðŸ“Š Live Market Data Feed",
        "type": "stat",
        "targets": [
          {
            "datasource": "Athena",
            "rawSql": "SELECT \n  COUNT(*) as total_updates,\n  COUNT(DISTINCT symbol) as active_symbols,\n  MAX(timestamp) as last_update\nFROM financial_data.processed_quotes \nWHERE timestamp >= current_timestamp - interval '1' minute",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 8
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
