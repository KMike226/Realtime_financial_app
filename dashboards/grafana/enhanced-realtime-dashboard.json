      {
        "id": 5,
        "title": "ðŸ’° Top Movers (Live)",
        "type": "table",
        "targets": [
          {
            "datasource": "Athena",
            "rawSql": "SELECT \n  symbol,\n  price,\n  change_percent,\n  volume,\n  timestamp,\n  CASE \n    WHEN change_percent > 0 THEN 'up'\n    WHEN change_percent < 0 THEN 'down'\n    ELSE 'neutral'\n  END as trend\nFROM financial_data.processed_quotes \nWHERE timestamp >= current_timestamp - interval '2' minute\nORDER BY ABS(change_percent) DESC\nLIMIT 10",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": true,
              "displayName": "Change %"
            }
          ]
        },
        "fieldConfig": {
          "defaults": {
