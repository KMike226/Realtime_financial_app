{
  "dashboard": {
    "id": null,
    "title": "Financial Platform - Real-time Overview",
    "tags": [
      "financial",
      "real-time",
      "platform",
      "overview"
    ],
    "style": "dark",
    "timezone": "America/New_York",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h"
      ],
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ]
    },
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "ðŸ“Š Platform Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"financial-platform\"}",
            "refId": "A",
            "legendFormat": "Platform Status"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "ðŸ”¥ Real-time Data Ingestion Rate",
        "type": "graph",
        "targets": [
          {
            "datasource": "CloudWatch",
            "expr": "sum(rate(kinesis_incoming_records_total[5m])) by (stream_name)",
            "refId": "A",
            "legendFormat": "{{stream_name}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "yAxes": [
          {
            "label": "Records/sec",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "xAxis": {
          "show": true
        },
        "lines": true,
        "fill": 2,
        "linewidth": 2,
        "points": false,
        "pointradius": 2,
        "bars": false,
        "stack": false,
        "percentage": false,
        "legend": {
          "avg": false,
          "current": false,
          "max": false,
          "min": false,
          "show": true,
          "total": false,
          "values": false
        },
        "nullPointMode": "null"
      },
      {
        "id": 3,
        "title": "ðŸ’° Top Stock Prices (Real-time)",
        "type": "table",
        "targets": [
          {
            "datasource": "Athena",
            "rawSql": "SELECT \n  symbol,\n  price,\n  change_percent,\n  volume,\n  timestamp\nFROM financial_data.processed_quotes \nWHERE timestamp >= current_timestamp - interval '5' minute\nORDER BY volume DESC\nLIMIT 10",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "styles": [
          {
            "alias": "Symbol",
            "align": "left",
            "colorMode": null,
            "colors": [
              "rgba(245, 54, 54, 0.9)",
              "rgba(237, 129, 40, 0.89)",
              "rgba(50, 172, 45, 0.97)"
            ],
            "decimals": 0,
            "pattern": "symbol",
            "thresholds": [],
            "type": "string",
            "unit": "short"
          },
          {
            "alias": "Price",
            "align": "right",
            "colorMode": null,
            "colors": [
              "rgba(245, 54, 54, 0.9)",
              "rgba(237, 129, 40, 0.89)",
              "rgba(50, 172, 45, 0.97)"
            ],
            "decimals": 2,
            "pattern": "price",
            "thresholds": [],
            "type": "number",
            "unit": "currencyUSD"
          },
          {
            "alias": "Change %",
            "align": "right",
            "colorMode": "cell",
            "colors": [
              "rgba(245, 54, 54, 0.9)",
              "rgba(237, 129, 40, 0.89)",
              "rgba(50, 172, 45, 0.97)"
            ],
            "decimals": 2,
            "pattern": "change_percent",
            "thresholds": [
              "-5",
              "0",
              "5"
            ],
            "type": "number",
            "unit": "percent"
          }
        ]
      },
      {
        "id": 4,
        "title": "âš¡ Lambda Processing Performance",
        "type": "graph",
        "targets": [
          {
            "datasource": "CloudWatch",
            "expr": "aws_lambda_duration_average{function_name=~\".*financial.*\"} / 1000",
            "refId": "A",
            "legendFormat": "{{function_name}} - Duration"
          },
          {
            "datasource": "CloudWatch",
            "expr": "aws_lambda_invocations_sum{function_name=~\".*financial.*\"}",
            "refId": "B",
            "legendFormat": "{{function_name}} - Invocations"
          }
        ],
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "yAxes": [
          {
            "label": "Duration (seconds)",
            "min": 0
          },
          {
            "label": "Invocations",
            "min": 0
          }
        ],
        "seriesOverrides": [
          {
            "alias": "/.*Invocations.*/",
            "yAxis": 2
          }
        ]
      },
      {
        "id": 5,
        "title": "ðŸš¨ Anomalies Detected (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "datasource": "Athena",
            "rawSql": "SELECT \n  COUNT(*) as anomaly_count,\n  anomaly_type,\n  severity\nFROM financial_data.anomalies \nWHERE timestamp >= current_timestamp - interval '1' hour\nGROUP BY anomaly_type, severity\nORDER BY anomaly_count DESC",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 18
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "sum"
            ]
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 20
                }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "ðŸ“ˆ Market Trends Analysis",
        "type": "graph",
        "targets": [
          {
            "datasource": "Athena",
            "rawSql": "SELECT \n  date_trunc('minute', timestamp) as time,\n  AVG(price) as avg_price,\n  symbol\nFROM financial_data.processed_quotes \nWHERE symbol IN ('AAPL', 'GOOGL', 'MSFT', 'AMZN', 'TSLA')\n  AND timestamp >= current_timestamp - interval '1' hour\nGROUP BY date_trunc('minute', timestamp), symbol\nORDER BY time",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 18
        },
        "yAxes": [
          {
            "label": "Price (USD)",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2
      },
      {
        "id": 7,
        "title": "ðŸ’¾ Data Storage Metrics",
        "type": "stat",
        "targets": [
          {
            "datasource": "CloudWatch",
            "expr": "aws_s3_bucket_size_bytes{bucket=~\".*financial.*\"} / 1024 / 1024 / 1024",
            "refId": "A",
            "legendFormat": "S3 Storage (GB)"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 18
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 10000000000
                },
                {
                  "color": "red",
                  "value": 50000000000
                }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "ðŸŽ¯ Technical Indicators - RSI",
        "type": "graph",
        "targets": [
          {
            "datasource": "Athena",
            "rawSql": "SELECT \n  timestamp as time,\n  rsi,\n  symbol\nFROM financial_data.technical_indicators \nWHERE symbol IN ('AAPL', 'GOOGL', 'MSFT')\n  AND timestamp >= current_timestamp - interval '2' hour\nORDER BY timestamp",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        },
        "yAxes": [
          {
            "label": "RSI",
            "min": 0,
            "max": 100
          },
          {
            "show": false
          }
        ],
        "thresholds": [
          {
            "value": 30,
            "colorMode": "critical",
            "op": "lt",
            "fill": true,
            "fillColor": "rgba(31, 120, 193, 0.2)",
            "line": true,
            "lineColor": "rgba(31, 120, 193, 0.6)"
          },
          {
            "value": 70,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "fillColor": "rgba(234, 112, 112, 0.2)",
            "line": true,
            "lineColor": "rgba(234, 112, 112, 0.6)"
          }
        ]
      },
      {
        "id": 9,
        "title": "ðŸ“Š Volume Analysis",
        "type": "graph",
        "targets": [
          {
            "datasource": "Athena",
            "rawSql": "SELECT \n  date_trunc('minute', timestamp) as time,\n  SUM(volume) as total_volume,\n  symbol\nFROM financial_data.processed_quotes \nWHERE symbol IN ('AAPL', 'GOOGL', 'MSFT', 'AMZN', 'TSLA')\n  AND timestamp >= current_timestamp - interval '1' hour\nGROUP BY date_trunc('minute', timestamp), symbol\nORDER BY time",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        },
        "yAxes": [
          {
            "label": "Volume",
            "min": 0,
            "logBase": 10
          },
          {
            "show": false
          }
        ],
        "bars": true,
        "lines": false,
        "fill": 8
      },
      {
        "id": 10,
        "title": "ðŸ”„ Data Pipeline Status",
        "type": "table",
        "targets": [
          {
            "datasource": "CloudWatch",
            "expr": "aws_kinesis_incoming_records{stream_name=~\".*financial.*\"}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        },
        "styles": [
          {
            "alias": "Component",
            "align": "left",
            "colorMode": null,
            "pattern": "Metric",
            "type": "string"
          },
          {
            "alias": "Status",
            "align": "center",
            "colorMode": "cell",
            "colors": [
              "rgba(245, 54, 54, 0.9)",
              "rgba(237, 129, 40, 0.89)",
              "rgba(50, 172, 45, 0.97)"
            ],
            "pattern": "Value",
            "thresholds": [
              "0.5",
              "0.8"
            ],
            "type": "number",
            "unit": "percentunit"
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
